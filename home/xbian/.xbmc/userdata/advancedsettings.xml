<advancedsettings>
   <network>
        <cachemembuffersize>20971520</cachemembuffersize>
   </network>
   <fanartres>720</fanartres>
   <imageres>540</imageres>
   <gputempcommand>echo "$(/opt/vc/bin/vcgencmd measure_temp | grep -o "[0-9]\{2\}") C"</gputempcommand>
