        <webserver>true</webserver>
        <guires>720</guires>
   </lookandfeel>
   <bginfoloadermaxthreads>2</bginfoloadermaxthreads>
   <video>
     <adjustrefreshrate>  
       <override>
         <fps>23.976</fps>
         <refresh>24.0</refresh>
       </override>
     </adjustrefreshrate>
   </video>
   <airplayport>7000</airplayport>
</advancedsettings>
